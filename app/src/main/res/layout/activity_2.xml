<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animationCache="true"

    android:background="@drawable/bgg"
    tools:context=".Activity2">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="275dp"
        android:layout_height="32dp"
        android:layout_marginTop="48dp"
        android:fontFamily="monospace"
        android:text="Kebutuhan Gizi"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="276dp"
        android:layout_height="57dp"
        android:layout_marginTop="104dp"
        android:fontFamily="monospace"
        android:text="Kebutuhan Gizi Seorang Berubah - ubah Seiring dengan pertumbuhannya"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="408dp"
        android:layout_height="54dp"
        android:src="@drawable/button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


    <Button
        android:id="@+id/button"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/square"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginBottom="14dp"
        android:background="@drawable/circle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:layout_width="20dp"
        android:layout_height="22dp"
        android:layout_marginStart="60dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/retangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button2"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="parent" />


    <Button
        android:id="@+id/buttonbalita"
        android:layout_width="185dp"
        android:layout_height="61dp"
        android:layout_marginTop="188dp"

        android:background="@drawable/bt"
        android:fontFamily="@font/ubuntu_bold"
        android:text="    BALITA"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView5" />




    <Button
        android:id="@+id/buttonanak"
        android:layout_width="185dp"
        android:layout_height="61dp"
        android:layout_marginTop="16dp"

        android:background="@drawable/bt"
        android:fontFamily="@font/ubuntu_bold"
        android:text="              ANAK ANAK"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonbalita" />

    <Button
        android:id="@+id/buttonremaja"
        android:layout_width="185dp"
        android:layout_height="61dp"

        android:layout_marginTop="16dp"
        android:background="@drawable/bt"
        android:fontFamily="@font/ubuntu_bold"
        android:text="      Remaja"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonanak" />

    <Button
        android:id="@+id/buttondewasa"
        android:layout_width="185dp"
        android:layout_height="61dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/bt"
        android:fontFamily="@font/ubuntu_bold"
        android:text="       Dewasa"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonremaja" />




</androidx.constraintlayout.widget.ConstraintLayout>
